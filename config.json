{
  "listen": "${HOST}",
  "web_root": "${ROOT}",
  "temp": "${TEMP}",
  "max_video_height": ${MAX_VIDEO_HEIGHT},
  "ffmpeg": {
    "ffmpeg": "${FFMPEG_BIN}",
    "ffprobe": "${FFPROBE_BIN}"
  },
  "s3": {
    "access_key": "${S3_APPKEY}",
    "secret_key": "${S3_SECRET}",
    "bucket": "${S3_BUCKET}",
    "region": "${S3_REGION}",
    "prefix": "/spin360",
    "vr360_prefix": "/vr360"
  },
  "aliyun-oss": {
    "access_key": "${OSS_APPKEY}",
    "secret_key": "${OSS_SECRET}",
    "bucket": "${OSS_BUCKET}",
    "endpoint": "${OSS_ENDPOINT}",
    "prefix": "spin360"
  }
}